<body>
    <a href="/">Home</a>
    <a href="/users">Users</a>
    <h1>New User</h1>
    <form action="/users" method="POST">
        <label for="name">Name</label>
        <input type="text" name="name" placeholder="Name" name="name">
        <label for="age">Age</label>
        <input type="text" name="age" placeholder="Age" name="age">
