<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Users Page</title>
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <h1>Users Page</h1>
    <p>Welcome to the users page</p>
     
    <div class="container">
        <% for(let user of users) { %>
            <div class="card">
                <p>Name: <%= user.name %></p>
                <p>Age: <%= user.age %></p>
                <p>Password: <%= user.password %></p>
                <p>City: <%= user.city %></p>
            </div>
        <% } %>
    </div>

</body>
</html>